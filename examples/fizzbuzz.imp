(import 'core/io')
(import 'core/operators')
(import 'core/control')

(def i 0)

(control:while {operators:<= i 100} {
	(def shouldPrintNumber 1)
	(control:if (operators:== 0 (operators:% i 3)) {
		(io:stdout:write 'Fizz')
		(set shouldPrintNumber 0)
	})
	(control:if (operators:== 0 (operators:% i 5)) {
		(io:stdout:write 'Buzz')
		(set shouldPrintNumber 0)
	})
	(control:if shouldPrintNumber {
		(io:stdout:write i)
	})
	(i:++)
	(io:stdout:writeLine)
})