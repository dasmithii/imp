#!/usr/bin/env imp

(import 'core/io')         
(import 'core/control' self)

(def i 1)

(while {i:<= 100} {
	(def shouldPrintNumber 1)
	(def m3 (i:% 3))
	(def m5 (i:% 5))
	(if (m3:== 0) {
		(io:write 'Fizz')
		(shouldPrintNumber:= 0)
	})
	(if (m5:== 0) {
		(io:write 'Buzz')
		(shouldPrintNumber:= 0)
	})
	(if shouldPrintNumber {
		(io:write i)
	})
	(i:++)
	(io:writeLine)
})