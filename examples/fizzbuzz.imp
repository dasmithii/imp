(import 'core/io')
(import 'core/operators')
(import 'core/control' self)

(def i 1)

(while {operators:<= i 1000} {
	(def shouldPrintNumber 1)
	(if (operators:== 0 (operators:% i 3)) {
		(io:stdout:write 'Fizz ')
		(shouldPrintNumber:set 0)
	})
	(if (operators:== 0 (operators:% i 5)) {
		(io:stdout:write 'Buzz')
		(shouldPrintNumber:set 0)
	})
	(if shouldPrintNumber {
		(io:stdout:write i)
	})
	(i:++)
})