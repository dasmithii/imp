#!/usr/bin/env imp


(import 'core/control' self)
(import 'core/io')
(import 'core/container/Vector')


(def max 100)
(io:writeLine 'Printing prime numbers below' max '...')
(io:writeLine 2)

(def primes (Vector:~))

(def check {
	(def n (@:at 0))
	(def i 0)
	(while {i:< primes:size} {
		(def p (primes:at i))
		(def mp (n:% p))
		(if (mp:!) {
			(return 0)
		})
		(i:++)
	})
	(return 1)
})

(def i 3)
(while {i:< 100} {
	(if (check i) {
		(primes:append (i:$))
		(io:writeLine i)
	})
	(i:+= 2)
})